<div>
    <h1>{{name}}</h1>
</div>
<hr>
<div>
    {{description}}
</div>
<div>
    {{#with history}}
          <div>{{year}}</div>
          <div>{{founder}}</div>
    {{/with}}
</div>
<br>
<h3>Types</h3>
<section>
    <ul>
        {{#each types}}
            <li>
                {{this}}
            </li>
        {{/each}}
    </ul>
</section>
<h3>Syntax</h3>
<section>
    <ul>
        {{#each syntax}}
            <li>
                {{this}}
            </li>
        {{/each}}
    </ul>
</section>
<h3>Features</h3>
<section>
    <ul>
        {{#each features}}
            <li>
                {{this}}
            </li>
        {{/each}}
    </ul>
</section>
<h3>Tools</h3>
<section>
    <ul>
        {{#each tools}}
            <li>
                {{this}}
            </li>
        {{/each}}
    </ul>
</section>
<h3>Community</h3>
<section>
    <ul>
        {{#each community}}
            <li>
                <div>{{name}}</div>
                <div>{{type}}</div>
            </li>
        {{/each}}
    </ul>
</section>